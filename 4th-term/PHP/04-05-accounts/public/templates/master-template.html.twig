            {#<li><a href="{{ item.href }}">{{ item.caption }}</a></li>#}
        {#{% endfor %}#}
    {#</ul>#}
{#{% endblock %}#}

{% block content %}{% endblock %}


<footer>
    {% block footer %}{% endblock %}
</footer>
</body>
</html>